message > Person:
  required > string: name: 1
  required > int32: id: 2
  optional > string: email: 3

  enum > PhoneType:
    MOBILE: 0
    HOME: 1
    WORK: 2
  end

  message > PhoneNumber:
    required > string: number: 1
    optional > PhoneType: type (default: HOME): 2
  end
  repeated > PhoneNumber: phone: 4
end